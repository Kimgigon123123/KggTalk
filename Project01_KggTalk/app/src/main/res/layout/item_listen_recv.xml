<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:layout_marginTop="10dp"
    android:layout_marginLeft="10dp"
    >

    <ImageView
        android:id="@+id/imgv_lsmusic1"
        android:scaleType="fitXY"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:src="@drawable/lsmusic1"
        android:alpha="0.8"
        />

    <TextView
        android:id="@+id/tv_lsmusic1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="음악감상"
        android:textStyle="bold"
        android:layout_alignBottom="@id/imgv_lsmusic1"
        android:layout_alignEnd="@id/imgv_lsmusic1"
        />
    <TextView
        android:layout_marginLeft="5dp"
        android:layout_marginTop="5dp"
        android:id="@+id/tv_lnTitle1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="조금 고독한 음악감상방"
        android:textSize="10dp"
        android:layout_below="@id/imgv_lsmusic1"
        />
    <ImageView
        android:layout_marginLeft="5dp"
        android:id="@+id/imgv_ln_character1"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:src="@drawable/ln_character1"
        android:layout_below="@id/tv_lnTitle1"
        />
    <TextView

        android:id="@+id/tv_lnPeople1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="16명"
        android:textSize="10dp"
        android:layout_toRightOf="@id/imgv_ln_character1"
        android:layout_below="@id/tv_lnTitle1"
        />
    <ImageView
        android:id="@+id/imgv_lnmusic1"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:src="@drawable/lnmusic1"
        android:layout_alignRight="@id/imgv_lsmusic1"
        android:layout_alignTop="@id/tv_lnTitle1"
        />

    <TextView
        android:layout_marginLeft="5dp"
        android:layout_marginTop="5dp"
        android:id="@+id/tv_lnTitle2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="오늘도 재즈 음악 감상"
        android:textSize="10dp"
        android:layout_below="@id/tv_lnPeople1"
        />
    <ImageView
        android:layout_marginLeft="5dp"
        android:id="@+id/imgv_ln_character2"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:src="@drawable/ln_character2"
        android:layout_below="@id/tv_lnTitle2"
        />
    <TextView
        android:id="@+id/tv_lnPeople2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="3명"
        android:textSize="10dp"
        android:layout_toRightOf="@id/imgv_ln_character2"
        android:layout_below="@id/tv_lnTitle2"
        />
    <ImageView
        android:id="@+id/imgv_lnmusic2"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:src="@drawable/lnmusic2"
        android:layout_alignRight="@id/imgv_lsmusic1"
        android:layout_alignTop="@id/tv_lnTitle2"
        />

    <TextView
        android:layout_marginLeft="5dp"
        android:layout_marginTop="5dp"
        android:id="@+id/tv_lnTitle3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="음악폭격기"
        android:textSize="10dp"
        android:layout_below="@id/tv_lnPeople2"
        />
    <ImageView
        android:layout_marginLeft="5dp"
        android:id="@+id/imgv_ln_character3"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:src="@drawable/ln_character3"
        android:layout_below="@id/tv_lnTitle3"
        />
    <TextView
        android:id="@+id/tv_lnPeople3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="5명"
        android:textSize="10dp"
        android:layout_toRightOf="@id/imgv_ln_character3"
        android:layout_below="@id/tv_lnTitle3"
        />
    <ImageView
        android:id="@+id/imgv_lnmusic3"
        android:layout_width="35dp"
        android:layout_height="25dp"
        android:src="@drawable/lnmusic3"
        android:layout_alignRight="@id/imgv_lsmusic1"
        android:layout_alignTop="@id/tv_lnTitle3"
        />
</RelativeLayout>